import java.util.*;
class A
{
	public void methodA()
	{
		System.out.println("welcome");
	}
}
class B extends A
{
	public void methodB()
	{
		System.out.println("to");
	}
}
class C extends B
{
	public void methodC()
	{
		System.out.println("java lab");
	}
	public static void main(String args[])
	{
		C obj = new C ();
		obj.methodA();
		obj.methodB();
		obj.methodC();
	}
}
